{
    "$id": "//trop/front/post_token_res",
    "body": {
        "type": "object",
        "additionalProperties": false,
        "required": ["type", "expires_in", "access_token", "refresh_token"],
        "properties": {
            "type": {
                "enum": ["bearer"]
            },
            "expires_in": {"$ref": "//atom/uint"},
            "access_token": {"$ref": "//atom/jwt"},
            "refresh_token": {"$ref": "//atom/uuidv4"}
        }
    }
}
